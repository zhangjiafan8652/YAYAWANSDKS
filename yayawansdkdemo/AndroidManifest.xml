        android:label="丫丫玩demoyyw"
        android:name="com.yayawan.proxy.YYWApplication"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.yayawan.sdkdemo.MainActivity"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize"
            android:label="丫丫玩demo"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- alipay sdk begin -->
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind"
            android:windowSoftInputMode="adjustResize|stateHidden" >
        </activity>

        <!-- BaseLogin_Activity 登录 -->
        <activity
            android:name="com.yayawan.sdk.jflogin.BaseLogin_Activity"
            android:configChanges="keyboardHidden|navigation|orientation|screenSize"
